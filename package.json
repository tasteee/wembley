{
  "name": "wembley",
  "type": "module",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "A delightful, DX-obsessed soundfont sequencer for the web. Chainable, expressive, and stupid-easy to get ideas flowing.",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "npm-run-all build-file copy",
    "build-file": "bun run build.ts",
    "copy": "cp dist examples/js",
    "prepublishOnly": "bun run build",
    "serve": "serve ./examples",
    "test": "bun test"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "audio",
    "soundfont",
    "sequencer",
    "music",
    "web-audio",
    "midi",
    "synthesizer",
    "piano",
    "chord",
    "note",
    "instrument"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tasteee/wembley#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasteee/wembley.git"
  },
  "bugs": "https://github.com/tasteee/wembley/issues",
  "author": "tasteee",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/bun": "^1.1.10",
    "@types/jest": "^30.0.0",
    "bun-plugin-dts": "^0.3.0",
    "esbuild": "^0.25.8",
    "jest": "^30.0.4",
    "playwright": "^1.54.1",
    "serve": "^14.2.4",
    "ts-jest": "^29.4.0"
  },
  "dependencies": {
    "tonal": "^6.4.2",
    "tone": "^15.1.22"
  }
}
