{
  "name": "wembley",
  "type": "module",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "A delightful, DX-obsessed soundfont sequencer for the web. Chainable, expressive, and stupid-easy to get ideas flowing.",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "node build-simple.cjs",
    "build-file": "node build-simple.cjs",
    "copy": "powershell -Command \"Copy-Item -Path 'dist/*' -Destination 'examples/js' -Recurse -Force\"",
    "prepublishOnly": "bun run build",
    "serve": "serve ./examples",
    "test": "bun test"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "audio",
    "soundfont",
    "sequencer",
    "music",
    "web-audio",
    "midi",
    "synthesizer",
    "piano",
    "chord",
    "note",
    "instrument"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tasteee/wembley#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasteee/wembley.git"
  },
  "bugs": "https://github.com/tasteee/wembley/issues",
  "author": "tasteee",
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@playwright/test": "^1.54.1",
    "@types/bun": "^1.1.10",
    "@types/jest": "^30.0.0",
    "esbuild": "^0.25.8",
    "jest": "^30.0.4",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.54.1",
    "serve": "^14.2.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "tonal": "^6.4.2",
    "tone": "^15.1.22"
  }
}
